<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <LangVersion>8.0</LangVersion>
    <Description>Provides System.Linq.IQueryable extensions such as filtering, ordering, paging, etc.</Description>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Company>ReHackt</Company>
    <Authors>Lionel Vallet</Authors>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/LionelVallet/ReHackt.Queryable.Extensions</PackageProjectUrl>
    <RepositoryUrl>https://github.com/LionelVallet/ReHackt.Queryable.Extensions</RepositoryUrl>
    <PackageReleaseNotes>https://github.com/LionelVallet/ReHackt.Queryable.Extensions/releases</PackageReleaseNotes>
    <PackageTags>Linq, Queryable, filtering, query syntax, parser</PackageTags>
    <Version>1.4.0</Version>
  </PropertyGroup>

</Project>
